{
  "name": "quicky-server",
  "version": "1.0.0",
  "description": "☂ A fast json server with no dependencies inspired in json-server",
  "author": "Kauê Fraga Rodrigues <kauefragarodrigues456@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/kauefraga/quicky-server.git",
  "publishConfig": {
    "registry":"https://registry.npmjs.org/"
  },
  "keywords": [
    "json-server",
    "api-rest"
  ],
  "main": "bin/cli/bin.js",
  "bin": {
    "@kauefraga/quicky-server": "bin/cli/bin.js",
    "quicky-server": "bin/cli/bin.js"
  },
  "files": [
    "bin/",
    "src/"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node .",
    "dev": "tsnd --respawn --transpile-only src/cli/bin.ts",
    "lint": "eslint"
  },
  "devDependencies": {
    "@types/node": "^18.8.5",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": ">=2.25.3 <3.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  }
}
